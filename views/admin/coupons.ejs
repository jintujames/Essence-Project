   <script>
  function deleteCoupon(id) {
  // Perform an AJAX request here
  Swal.fire({
    title: 'Are you sure?',
    text: "You won't be able to revert this!",
    icon: 'warning',
    showCancelButton: true,
    confirmButtonColor: '#3085d6',
    cancelButtonColor: '#d33',
    confirmButtonText: 'Yes, delete it!'
  }).then((result) => {
    if (result.isConfirmed) {
      $.ajax({
        url: '/admin/delete-coupon',
        type: 'POST',
        data: {
          couponId: id
        },
        success: function (response) {
          // Handle the success response here
          if (response.status) {
            Swal.fire(
              'Deleted!',
              'Your file has been deleted.',
              'success'
            ).then(() => {
              location.reload();
            });
          }
        },
        error: function (xhr, status, error) {
          // Handle errors here
          console.error('AJAX request error:', error);
        }
      });
    }
  });
}

$(document).ready(function () {
  // Form submission to create a coupon
  $("#create-coupon-form").submit(function (event) {
    event.preventDefault();
    $.ajax({
      url: "/admin/coupons",
      type: "POST",
      data: $(this).serialize(),
      dataType: "json",
      success: function (response) {
        if (response.status) {
          Swal.fire("Coupon created!", "Your new coupon has been created!", "success").then(() => {
            location.reload();
          });
        } else {
          alert(response.message);
        }
      },
      error: function (jqXHR, textStatus, errorThrown) {
        alert("Error creating coupon: " + textStatus);
      },
    });
  });
});


   </script>

