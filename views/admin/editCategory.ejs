            </div>
            
            
            <form class="row contact_form" action="/admin/editCategory" method="post" onsubmit="return validate()">
                <div class="row">
                    <div class="col-md-6">
                        <input list="search_terms" type="text" id="name" name="name" class="form-control form-control-sm" value="<%- category.name %>">
                        <input type="hidden" name="id" value="<%- category._id %>">
                    </div>
                    <div class="col-md-6 text-md-middle"> <!-- Adjust the column width as needed -->
                        <button type="submit" class="btn btn-primary"></i>Update</button>
                    </div>
                </div>
                
            </form>

