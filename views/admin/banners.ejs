        <section class="content-main">
            <div class="content-header">
                <h2 class="content-title">BANNER</h2>
                <div>
                    <a href="/admin/addBanner" class="btn btn-primary"><i class="material-icons md-plus"></i>Add Banner</a>
                </div>
            </div>
            <div class="card mb-4">
                <div class="card-body">
                    <div class="table-responsive">
                        <table class="table table-hover">
                            <thead>
                                <tr>
                                    <th>Image</th>
                                    <th>Banner Head</th>
                                    <th>Banner Text</th>
                                    <th>Status</th>
                                    <th>Delete</th>
                                </tr>
                            </thead>
                            <tbody>
                                <% banners.forEach((banner, i) => { %>
                                    <tr>
                                        <td>
                                            <div class="d-flex align-items-center">
                                                <img style="max-width: 100px; max-height: 100px; width: auto; height: auto;" src="/admin/assets/bannerImages/<%= banner.image[0] %>" class="img-l img-fluid" alt="Banner Image">
                                            </div>
                                        </td>
                                        <td>
                                            <%= banner.bannerHead %>
                                        </td>
                                        <td>
                                            <%= banner.bannerText %>
                                        </td>
                                        <td>
                                            <form action="/admin/block-banner" method="post">
                                                <div class="form-check form-switch">
                                                    <input
                                                        class="form-check-input"
                                                        type="checkbox"
                                                        id="flexSwitchCheckActive<%= banner._id %>"
                                                        onchange="this.form.submit()"
                                                        <%= banner.isActive ? 'checked' : '' %>
                                                    >
                                                    <label class="form-check-label" for="flexSwitchCheckActive<%= banner._id %>">
                                                        <%= banner.name %>
                                                    </label>
                                                    <input type="hidden" value="<%= banner._id %>" name="id"> <!-- Use 'id' for the banner ID -->
                                                </div>
                                            </form>
                                            
                                        </td>
                                        
                                        
                                        <td class="text">
                                            <!-- Add your delete button here -->
                                            <button id="deleteBanner" onclick="deleteBanner('<%= banner._id %>')" class="btn btn-danger">Delete</button>
                                        </td>
                                        
                                    </tr>
                                <% }); %>
                                <% if (banners.length === 0) { %>
                                    <tr>
                                        <td colspan="6">
                                            <h1 class="not-found">No banners found.</h1>
                                        </td>
                                    </tr>
                                <% } %>
                            </tbody>
                        </table>
                        <!-- table-responsive.// -->
