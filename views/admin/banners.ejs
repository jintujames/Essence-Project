    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/sweetalert2@11.0.20/dist/sweetalert2.min.css">
<script src="https://cdn.jsdelivr.net/npm/sweetalert2@11.0.20/dist/sweetalert2.all.min.js"></script>


    <script>
        function deleteBanner(bannerId) {
            Swal.fire({
                title: 'Delete Banner',
                text: 'Are you sure you want to delete this banner?',
                icon: 'warning',
                showCancelButton: true,
                confirmButtonColor: '#d33',
                cancelButtonColor: '#3085d6',
                confirmButtonText: 'Yes, delete it!',
            }).then((result) => {
                if (result.isConfirmed) {
                    // Send a DELETE request to your server
                    fetch(`/admin/delete-banner?id=${bannerId}`, {
                        method: 'DELETE',
                    })
                        .then((response) => {
                            if (response.status === 200) {
                                Swal.fire('Deleted!', 'The banner has been deleted.', 'success');
                                // Reload the page or update the banner list
                                location.reload();
                            } else {
                                Swal.fire('Error', 'Failed to delete the banner.', 'error');
                            }
                        })
                        .catch((error) => {
                            console.error('Error:', error);
                            Swal.fire('Error', 'An error occurred while deleting the banner.', 'error');
                        });
                }
            });
        }
    </script>
    
    
